<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.searchandrescue_ui.MainActivity"
    android:background="@drawable/mainbg">

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="329dp"
        android:layout_height="77dp"
        android:layout_marginBottom="178dp"
        android:layout_marginTop="317dp"
        android:adjustViewBounds="true"
        android:backgroundTint="@android:color/transparent"
        android:onClick="sendRescue"
        android:scaleType="fitCenter"
        android:src="@drawable/rescuemev2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.553"
        app:srcCompat="@drawable/rescuemev2"
        tools:layout_editor_absoluteY="286dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="336dp"
        android:layout_height="75dp"
        android:layout_marginBottom="58dp"
        android:adjustViewBounds="true"
        android:backgroundTint="@android:color/transparent"
        android:onClick="sendWillRescue"
        android:scaleType="fitCenter"
        android:src="@drawable/illrescuev2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/illrescuev2" />

</android.support.constraint.ConstraintLayout>
